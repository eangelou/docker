{
    "id": "/eangelou",
    "cpus": 1.5,
    "mem": 1024.0,
    "disk": 1024.0,
    "requirePorts": true,
    "ports": [
       32022,
       32122
    ],
    "instances": 1,
    "executor": "",
    "container": {
  "type": "DOCKER",
      "id":"memcached",
     "instances": 1,
    "cpus": 0.5,
    "mem": 512,
  "docker": {
    "image": "eangelou/cslab",
    "network": "BRIDGE",
    "portMappings": [
      {
        "containerPort": 22,
        "hostPort": 31022,
        "servicePort": 32022,
        "protocol": "tcp"
      },
      { 
        "containerPort": 11211,
        "hostPort": 31122,
        "servicePort": 32122,
        "protocol": "tcp"
      }
    ],
    "privileged": false,
    "parameters": [
      {
        "key": "cpuset-cpus",
        "value": "0,1"
      },
      {
        "key": "cpu-shares",
        "value": "512"
      }
    ],
    "forcePullImage": true
    }
}
}

